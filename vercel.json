{
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.example.com/",
      "permanent": true,
      "experimental": {
        "optimizeCss": false
      },
      "has": [
        {
          "type": "host",
          "value": "example.com"
        }
      ]
    },
    {
      "source": "/:path*",
      "destination": "https://example.com/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.example.com"
        }
      ]
    }
  ]
}